  ._lm .msg_item .thumbs_list4 .thumb_item, ._lm .post_item .thumbs_list4 .thumb_item, ._lm .wall_item .thumbs_list4 .thumb_item { width: 24%; max-height: none; margin: 0px 3px 3px 0px; }
}

@media screen and (min-width: 270px) {
  ._lm .msg_item .thumbs_list3 .thumb_item, ._lm .post_item .thumbs_list3 .thumb_item, ._lm .wall_item .thumbs_list3 .thumb_item { width: 32%; max-height: none; margin: 0px 3px 3px 0px; }
}

@media screen and (min-width: 140px) {
  ._lm .msg_item .thumbs_list2 .thumb_item, ._lm .post_item .thumbs_list2 .thumb_item, ._lm .wall_item .thumbs_list2 .thumb_item { width: 48%; max-height: none; margin: 0px 4px 4px 0px; }
}

@media screen {
  ._lm .msg_item .thumbs_list1 .thumb_item, ._lm .post_item .thumbs_list1 .thumb_item, ._lm .wall_item .thumbs_list1 .thumb_item { width: auto; max-height: none; margin: 0px 0px 4px; }
}

