.Unfold { position: relative; user-select: none; min-width: 44px; min-height: 44px; }

.Unfold__popup { min-width: 100px; position: absolute; z-index: 10; left: 4px; top: 4px; background: rgb(255, 255, 255); overflow: hidden auto; border-radius: 8px; border: 1px solid rgb(222, 224, 227); display: none; max-height: calc(100vh - 195px); }

.basis__header .Unfold__popup { max-width: 100%; }

.vk_js_no .Unfold__popup { top: 43px; }

.Unfold_open .Unfold__popup, .vk_js_no .Unfold_noJSopen .Unfold__popup { display: block; }

.Unfold_position_topRight .Unfold__popup { left: auto; right: 4px; }

.Unfold_position_bottomRight .Unfold__popup { left: auto; right: 4px; bottom: 4px; top: auto; }

.Unfold_position_bottomLeft .Unfold__popup { bottom: 4px; top: auto; }

@supports ((-webkit-box-shadow: initial) or (-moz-box-shadow: initial) or (box-shadow: initial)) {
  .Unfold__popup { border: none; box-shadow: rgba(0, 0, 0, 0.12) 0px 1px 3px 1px; }
}

@supports ((-webkit-transition: initial) or (-moz-transition: initial) or (-o-transition: initial) or (transition: initial)) {
  .Unfold__popup { display: block; visibility: hidden; opacity: 0; transform: translateY(-10px); transition: visibility 0s linear 0.2s, transform 0.2s ease 0s, opacity 0.2s ease 0s, -webkit-transform 0.2s ease 0s, -moz-transform 0.2s ease 0s, -o-transform 0.2s ease 0s; }
